					<filepath>Resource/CalDAV/rscale/bad/2.ics</filepath>
				</data>
				<verify>
					<callback>prepostcondition</callback>
					<arg>
						<name>error</name>
						<value>{urn:ietf:params:xml:ns:caldav}valid-calendar-data</value>
					</arg>
				</verify>
			</request>
		</test>
	</test-suite>

